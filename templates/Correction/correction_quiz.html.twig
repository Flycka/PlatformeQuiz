{% extends 'base.html.twig' %}

{% block body %}
    <h1>Correction Quiz</h1>
    <p>Quiz: {{ quiz.titre }}</p>

    <table>
        <thead>
        <tr>
            <th>Question</th>
            <th>Note maximale</th>
            <th>Réponse étudiant</th>
            <th>Commentaire</th>
            <th>Note</th>
        </tr>
        </thead>
        <tbody>
        {% for question in questions %}
            <tr>
                <td>{{ question.question }}</td>
                <td>{{ question.noteMaximalequestion }}</td>


            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}